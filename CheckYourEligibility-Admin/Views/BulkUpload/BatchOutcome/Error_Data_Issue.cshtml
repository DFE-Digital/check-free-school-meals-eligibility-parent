@{
    ViewData["Title"] = "Could not check eligibility";
}

@* <a class="govuk-back-link" href="#" onclick="history.back(); return false;">Back</a> *@
<a class="govuk-back-link-nolink"></a>

<div class="govuk-error-summary" data-module="govuk-error-summary">
    <div role="alert">
        <h2 class="govuk-error-summary__title">
            Error – Parent data issue
        </h2>
        <div class="govuk-error-summary__body">
            <ul class="govuk-list govuk-error-summary__list">
                <li>
                    <strong>There are issues with the parent data on the following lines:</strong>
                </li>
                @Html.Raw(@TempData["BatchParentCheckItemsErrors"])

                @if (Convert.ToInt32(@TempData["BatchParentCheckItemsLineMoreErrors"]) > 0)
                    {
                    <li>
                        <strong>There are also issues on @TempData["BatchParentCheckItemsLineMoreErrors"] more lines</strong>
                    </li>
                    }
                    
                
            </ul>
        </div>
    </div>
</div>
<p class="govuk-body">Please make sure you’ve corrected the issues on these lines and upload the file again.</p>
<p>@Html.ActionLink("Try again", "Batch_Check", "BulkUpload")</p>
<p><a href="@Url.Action("Index", "Home")">Return to dashboard</a></p>
<p>@Html.ActionLink("Sign Out", "SignOut", "Account")</p>
