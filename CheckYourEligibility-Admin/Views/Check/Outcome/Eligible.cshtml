@* <a class="govuk-back-link" href="#" onclick="history.back(); return false;">Back</a>*@
<a class="govuk-back-link-nolink"></a>

<div class="govuk-grid-row" id="content" data-type="Eligible">
    <div class="govuk-grid-column-full">
        <h1 class="govuk-heading-l">@TempData["OutcomeText"]</h1>


        <h2 class="govuk-heading-m">What to do next</h2>

        <p>Select 'Add children's details' on this page and provide the information requested.</p>

        <form action="Enter_Child_Details" novalidate>
            <button class="govuk-button govuk-!-margin-top-5" data-module="govuk-button">
                Add children's details
            </button>
        </form>

        <p> @Html.ActionLink("Return to dashboard", "index", "home")</p>
        <p> @Html.ActionLink("Sign Out", "SignOut", "Account")</p>
    </div>
</div>